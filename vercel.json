{
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Content-Security-Policy",
            "value": "frame-ancestors *; default-src https:"
          },
          {
            "key": "X-Frame-Options",
            "value": "ALLOWALL"
          }
        ]
      }
    ]
  }
  